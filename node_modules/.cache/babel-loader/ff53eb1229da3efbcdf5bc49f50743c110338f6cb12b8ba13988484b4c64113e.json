{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, reactive } from 'vue';\nimport { dealData } from '@/utils/util.js';\nimport Different from '@/components/different.vue';\nexport default {\n  __name: 'noCollapseTable',\n  props: {\n    title: {\n      type: String,\n      default: ''\n    },\n    columns: {\n      type: Array,\n      default: () => []\n    },\n    setColumnsData: {\n      type: Array,\n      default: () => []\n    },\n    // 表头\n    data: {\n      type: Array,\n      default: () => []\n    },\n    loading: {\n      type: Boolean,\n      default: false\n    },\n    expandedRowKeys: {\n      type: Array,\n      default: () => []\n    }\n  },\n  emits: [\"noexpandedChange\"],\n  setup(__props, {\n    expose,\n    emit\n  }) {\n    expose();\n    const props = __props;\n    const modalVisible = ref(false);\n    const setVisible = ref(false);\n    const setColumnsList = ref([]);\n    const defaultCheckList = ref([]);\n    const dataSource = ref(props.data);\n    const expandedRowKeys = ref(props.expandedRowKeys);\n    const popupVisible = ref(false);\n    console.log(props.data, 'dataSource');\n    //单行高亮处理\n    const activeRowClass = record => {\n      if (record.raw.isGreenLight) {\n        return 'activeRowGreen';\n      } else if (record.raw.isRate) {\n        return 'activeRowPurple';\n      } else {\n        return '';\n      }\n    };\n    // 打开环比差\n    const handleDifference = () => {\n      modalVisible.value = true;\n    };\n    // 关闭\n    const handleClose = () => {\n      modalVisible.value = false;\n    };\n    // 点击数据设置\n    const handleSetting = () => {\n      setColumnsList.value = [];\n      defaultCheckList.value = [];\n      setVisible.value = true;\n      props.setColumnsData.forEach(item => {\n        setColumnsList.value.push({\n          id: item.key,\n          analysis: item.analysis\n        });\n      });\n      dataSource.value.forEach(item => {\n        setColumnsList.value.forEach(ele => {\n          if (item.key === ele.id) {\n            defaultCheckList.value.push(item.key);\n          }\n        });\n      });\n    };\n    // 更改数据设置\n    const changeSet = val => {\n      defaultCheckList.value = val;\n    };\n\n    // 已展开的数据行发生改变时触发\n    const expandedChange = rowkeys => {\n      expandedRowKeys.value = rowkeys;\n      emit(\"noexpandedChange\", expandedRowKeys.value);\n    };\n    // 数据设置确定\n    const handleOk = () => {\n      let arr = [];\n      dataSource.value = props.data;\n      props.setColumnsData.forEach(item => {\n        if (defaultCheckList.value.indexOf(item.key) !== -1) {\n          arr.push(item);\n        }\n      });\n      dataSource.value = arr.concat(dataSource.value);\n      expandedRowKeys.value = dealData(dataSource.value).keyArr;\n      popupVisible.value = false;\n    };\n    const handleCancle = () => {\n      popupVisible.value = false;\n    };\n    const __returned__ = {\n      props,\n      modalVisible,\n      setVisible,\n      setColumnsList,\n      defaultCheckList,\n      dataSource,\n      expandedRowKeys,\n      popupVisible,\n      activeRowClass,\n      handleDifference,\n      handleClose,\n      handleSetting,\n      changeSet,\n      emit,\n      expandedChange,\n      handleOk,\n      handleCancle,\n      ref,\n      reactive,\n      get dealData() {\n        return dealData;\n      },\n      Different\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":";AAgFA,SAASA,GAAG,EAACC,QAAQ,QAAO,KAAK;AACjC,SAAQC,QAAQ,QAAO,iBAAiB;AACxC,OAAOC,SAAS,MAAO,4BAA4B;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;IA4BnD,MAAMC,YAAY,GAAGJ,GAAG,CAAC,KAAK,CAAC;IAC/B,MAAMK,UAAU,GAAGL,GAAG,CAAC,KAAK,CAAC;IAC7B,MAAMM,cAAc,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMO,gBAAgB,GAAGP,GAAG,CAAC,EAAE,CAAC;IAChC,MAAMQ,UAAU,GAAIR,GAAG,CAACS,KAAK,CAACC,IAAI,CAAC;IACnC,MAAMC,eAAe,GAAGX,GAAG,CAACS,KAAK,CAACE,eAAe,CAAC;IAClD,MAAM